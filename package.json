{
  "name": "igniteui-i18n",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:core && npm run build:resources && npm run build:angular",
    "build:core": "tsc --project projects/igniteui-i18n-core/tsconfig.build.json && @powershell copy \"projects/igniteui-i18n-core/package.json\"  \"dist/igniteui-i18n-core/package.json\"",
    "build:resources": "tsc --project projects/igniteui-i18n-resources/tsconfig.build.json && @powershell copy \"projects/igniteui-i18n-resources/package.json\"  \"dist/igniteui-i18n-resources/package.json\"",
    "build:angular": "tsc --project projects/igniteui-angular-i18n/tsconfig.build.json && @powershell copy \"projects/igniteui-angular-i18n/package.json\"  \"dist/igniteui-angular-i18n/package.json\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.0.0"
  }
}
